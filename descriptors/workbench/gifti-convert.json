{
  "tool-version": "1.5.0 2e663bdff57597d5d1fb251dc4740d14d9cf11dd",
  "name": "gifti-convert",
  "author": "Washington University School of Medicin",
  "command-line": "wb_command -gifti-convert [GIFTI-ENCODING] [INPUT-GIFTI-FILE] [OUTPUT-GIFTI-FILE]",
  "container-image": {
    "image": "mcin/docker-fsl:latest",
    "type": "docker"
  },
  "description": "CONVERT A GIFTI FILE TO A DIFFERENT ENCODING.\n\nThe value of <gifti-encoding> must be one of the following:\n\nASCII\nBASE64_BINARY\nGZIP_BASE64_BINARY\nEXTERNAL_FILE_BINARY",
  "schema-version": "0.5",
  "tags": {},
  "inputs": [
    {
      "id": "gifti-encoding",
      "name": "gifti-encoding",
      "description": "what the output encoding should be",
      "type": "String",
      "optional": false,
      "value-key": "[GIFTI-ENCODING]"
    },
    {
      "id": "input-gifti-file",
      "name": "input-gifti-file",
      "description": "the input gifti file",
      "type": "String",
      "optional": false,
      "value-key": "[INPUT-GIFTI-FILE]"
    },
    {
      "id": "output-gifti-file",
      "name": "output-gifti-file",
      "description": "output - the output gifti file",
      "type": "String",
      "optional": false,
      "value-key": "[OUTPUT-GIFTI-FILE]"
    }
  ]
}