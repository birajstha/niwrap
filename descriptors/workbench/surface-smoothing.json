{
  "tool-version": "1.5.0 2e663bdff57597d5d1fb251dc4740d14d9cf11dd",
  "name": "surface-smoothing",
  "author": "Washington University School of Medicin",
  "command-line": "wb_command -surface-smoothing [SURFACE-IN] [SMOOTHING-STRENGTH] [SMOOTHING-ITERATIONS]",
  "container-image": {
    "image": "mcin/docker-fsl:latest",
    "type": "docker"
  },
  "description": "SURFACE SMOOTHING.\n\nSmooths a surface by averaging vertex coordinates with those of the neighboring vertices.",
  "schema-version": "0.5",
  "tags": {},
  "inputs": [
    {
      "id": "surface-in",
      "name": "surface-in",
      "description": "the surface file to smooth",
      "type": "File",
      "optional": false,
      "value-key": "[SURFACE-IN]"
    },
    {
      "id": "smoothing-strength",
      "name": "smoothing-strength",
      "description": "smoothing strength (ranges [0.0 - 1.0])",
      "type": "Number",
      "optional": false,
      "value-key": "[SMOOTHING-STRENGTH]"
    },
    {
      "id": "smoothing-iterations",
      "name": "smoothing-iterations",
      "description": "smoothing iterations",
      "type": "Integer",
      "optional": false,
      "value-key": "[SMOOTHING-ITERATIONS]"
    },
    {
      "id": "surface_out",
      "name": "surface_out",
      "description": "output surface file",
      "type": "File",
      "optional": false,
      "value-key": "[SURFACE_OUT]"
    }
  ],
  "output-files": [
    {
      "id": "surface_out",
      "name": "surface_out",
      "path-template": "[SURFACE_OUT]",
      "description": "output surface file",
      "optional": false
    }
  ]
}
