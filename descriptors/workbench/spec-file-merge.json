{
  "tool-version": "1.5.0 2e663bdff57597d5d1fb251dc4740d14d9cf11dd",
  "name": "spec-file-merge",
  "author": "Washington University School of Medicin",
  "command-line": "wb_command -spec-file-merge [SPEC-1] [SPEC-2] [OUT-SPEC]",
  "container-image": {
    "image": "mcin/docker-fsl:latest",
    "type": "docker"
  },
  "description": "MERGE TWO SPEC FILES INTO ONE.\n\nThe output spec file contains every file that is in either of the input spec files.",
  "schema-version": "0.5",
  "tags": {},
  "inputs": [
    {
      "id": "spec-1",
      "name": "spec-1",
      "description": "first spec file to merge",
      "type": "String",
      "optional": false,
      "value-key": "[SPEC-1]"
    },
    {
      "id": "spec-2",
      "name": "spec-2",
      "description": "second spec file to merge",
      "type": "String",
      "optional": false,
      "value-key": "[SPEC-2]"
    },
    {
      "id": "out-spec",
      "name": "out-spec",
      "description": "output - output spec file",
      "type": "String",
      "optional": false,
      "value-key": "[OUT-SPEC]"
    }
  ]
}