{
  "tool-version": "1.5.0 2e663bdff57597d5d1fb251dc4740d14d9cf11dd",
  "name": "volume-create",
  "author": "Washington University School of Medicin",
  "command-line": "wb_command -volume-create [I_DIM] [J_DIM] [K_DIM] [VOLUME_OUT] [PLUMB] [SFORM]",
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  },
  "description": "Create a blank volume file.\n\nCreates a volume file full of zeros.  Exactly one of -plumb or -sform must be specified.",
  "schema-version": "0.5",
  "tags": {},
  "inputs": [
    {
      "id": "i_dim",
      "name": "i_dim",
      "description": "length of first dimension",
      "optional": false,
      "value-key": "[I_DIM]",
      "type": "Number",
      "integer": true
    },
    {
      "id": "j_dim",
      "name": "j_dim",
      "description": "length of second dimension",
      "optional": false,
      "value-key": "[J_DIM]",
      "type": "Number",
      "integer": true
    },
    {
      "id": "k_dim",
      "name": "k_dim",
      "description": "length of third dimension",
      "optional": false,
      "value-key": "[K_DIM]",
      "type": "Number",
      "integer": true
    },
    {
      "id": "volume_out",
      "name": "volume_out",
      "description": "the output volume",
      "optional": false,
      "value-key": "[VOLUME_OUT]",
      "type": "File"
    },
    {
      "id": "plumb",
      "name": "plumb",
      "description": "set via axis order and spacing/offset",
      "command-line-flag": "-plumb",
      "type": {
        "id": "plumb",
        "name": "plumb",
        "description": "set via axis order and spacing/offset",
        "command-line": ""
      },
      "optional": true,
      "value-key": "[PLUMB]",
      "list": false
    },
    {
      "id": "sform",
      "name": "sform",
      "description": "set via a nifti sform",
      "command-line-flag": "-sform",
      "type": {
        "id": "sform",
        "name": "sform",
        "description": "set via a nifti sform",
        "command-line": ""
      },
      "optional": true,
      "value-key": "[SFORM]",
      "list": false
    }
  ],
  "output-files": [
    {
      "id": "volume_out",
      "name": "volume_out",
      "path-template": "[VOLUME_OUT]",
      "description": "the output volume",
      "optional": false
    }
  ]
}
