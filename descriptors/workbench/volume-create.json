{
  "tool-version": "1.5.0 2e663bdff57597d5d1fb251dc4740d14d9cf11dd",
  "name": "volume-create",
  "author": "Washington University School of Medicin",
  "command-line": "wb_command -volume-create [I-DIM] [J-DIM] [K-DIM]",
  "container-image": {
    "image": "mcin/docker-fsl:latest",
    "type": "docker"
  },
  "description": "CREATE A BLANK VOLUME FILE.\n\nCreates a volume file full of zeros.  Exactly one of -plumb or -sform must be specified.",
  "schema-version": "0.5",
  "tags": {},
  "inputs": [
    {
      "id": "i-dim",
      "name": "i-dim",
      "description": "length of first dimension",
      "type": "Integer",
      "optional": false,
      "value-key": "[I-DIM]"
    },
    {
      "id": "j-dim",
      "name": "j-dim",
      "description": "length of second dimension",
      "type": "Integer",
      "optional": false,
      "value-key": "[J-DIM]"
    },
    {
      "id": "k-dim",
      "name": "k-dim",
      "description": "length of third dimension",
      "type": "Integer",
      "optional": false,
      "value-key": "[K-DIM]"
    },
    {
      "id": "volume_out",
      "name": "volume_out",
      "description": "the output volume",
      "type": "File",
      "optional": false,
      "value-key": "[VOLUME_OUT]"
    }
  ],
  "output-files": [
    {
      "id": "volume_out",
      "name": "volume_out",
      "path-template": "[VOLUME_OUT]",
      "description": "the output volume",
      "optional": false
    }
  ]
}
