{
  "tool-version": "1.0",
  "name": "GetAfniID",
  "author": "AFNI Development Team",
  "description": "Returns the unique identifier of a dataset.",
  "url": "https://afni.nimh.nih.gov/",
  "command-line": "GetAfniID [DSET]",
  "container-image": {
    "image": "afni/afni:latest",
    "index": "index.docker.io",
    "type": "docker"
  },
  "inputs": [
    {
      "description": "Dataset for which the unique identifier is to be returned.",
      "value-key": "[DSET]",
      "type": "File",
      "optional": false,
      "id": "dset",
      "name": "Dataset"
    }
  ],
  "schema-version": "0.5",
  "output-files": [
    {
      "path-template": "stdout",
      "description": "Unique identifier of the dataset",
      "optional": false,
      "id": "unique_id",
      "name": "Unique ID"
    }
  ]
}
