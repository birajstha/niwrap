{
  "name": "3dAutomask",
  "command-line": "3dAutomask [IN_FILE] [ARGS] [BRAIN_FILE] [CLFRAC] [DILATE] [ENVIRON] [ERODE] [NUM_THREADS] [OUT_FILE] [OUTPUTTYPE]",
  "author": "Nipype (interface)",
  "description": "Automask, as implemented in Nipype (module: nipype.interfaces.afni.preprocess, interface: Automask).\nCreate a brain-only mask of the image using AFNI 3dAutomask command\nFor complete details, see the `3dAutomask Documentation. <https://afni.nimh.nih.gov/pub/dist/doc/program_help/3dAutomask.html>`_",
  "inputs": [
    {
      "id": "clfrac",
      "name": "Clfrac",
      "type": "Number",
      "value-key": "[CLFRAC]",
      "command-line-flag": "-clfrac",
      "description": "Sets the clip level fraction (must be 0.1-0.9). a small value will tend to make the mask larger [default = 0.5].",
      "optional": true
    },
    {
      "id": "dilate",
      "name": "Dilate",
      "type": "Number",
      "integer": true,
      "value-key": "[DILATE]",
      "command-line-flag": "-dilate",
      "description": "Dilate the mask outwards.",
      "optional": true
    },
    {
      "id": "erode",
      "name": "Erode",
      "type": "Number",
      "integer": true,
      "value-key": "[ERODE]",
      "command-line-flag": "-erode",
      "description": "Erode the mask inwards.",
      "optional": true
    },
    {
      "id": "in_file",
      "name": "In file",
      "type": "File",
      "value-key": "[IN_FILE]",
      "description": "Input file to 3dautomask.",
      "optional": false
    },
    {
      "id": "num_threads",
      "name": "Num threads",
      "type": "Number",
      "integer": true,
      "value-key": "[NUM_THREADS]",
      "description": "Set number of threads.",
      "optional": true,
      "default-value": 1
    },
    {
      "id": "outputtype",
      "name": "Outputtype",
      "type": "String",
      "value-key": "[OUTPUTTYPE]",
      "description": "'nifti' or 'afni' or 'nifti_gz'. Afni output filetype.",
      "optional": true,
      "value-choices": ["NIFTI", "AFNI", "NIFTI_GZ"]
    }
  ],
  "output-files": [
    {
      "name": "Brain file",
      "id": "brain_file",
      "optional": true,
      "description": "Output file from 3dautomask.",
      "path-template": "[IN_FILE]_masked",
      "value-key": "[BRAIN_FILE]",
      "command-line-flag": "-apply_prefix"
    },
    {
      "name": "Out file",
      "id": "out_file",
      "optional": true,
      "description": "Output image file name.",
      "path-template": "[IN_FILE]_mask",
      "value-key": "[OUT_FILE]",
      "command-line-flag": "-prefix"
    },
    {
      "name": "Brain file",
      "id": "brain_file",
      "path-template": "brain_file",
      "optional": true,
      "description": "Brain file (skull stripped)."
    },
    {
      "name": "Out file",
      "id": "out_file",
      "path-template": "out_file",
      "optional": true,
      "description": "Mask file."
    }
  ],
  "tool-version": "1.0.0",
  "schema-version": "0.5",
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  },
  "tags": {
    "domain": "neuroinformatics",
    "source": "nipype-interface"
  }
}
