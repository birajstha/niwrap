{
  "tool-version": "1.0",
  "name": "@ClustExp_run_shiny",
  "author": "Justin Rajendra",
  "description": "Launch a shiny app that was created by ClustExp_StatParse.py",
  "url": "https://example.com/clustexp_run_shiny",
  "command-line": "@ClustExp_run_shiny [DIRECTORY] [HELP_FLAG]",
  "container-image": {
    "image": "yourdockerhubusername/clustexp_run_shiny:latest",
    "index": "index.docker.io",
    "type": "docker"
  },
  "inputs": [
    {
      "description": "Folder created by ClustExp_StatParse.py",
      "value-key": "[DIRECTORY]",
      "type": "String",
      "optional": false,
      "id": "directory",
      "name": "Input Directory"
    },
    {
      "command-line-flag": "-help",
      "description": "Show help message",
      "value-key": "[HELP_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "help",
      "name": "Help Flag"
    }
  ],
  "schema-version": "0.5"
}
