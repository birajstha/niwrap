{
  "tool-version": "1.0",
  "name": "eg_main_chrono",
  "author": "AFNI Team",
  "description": "A sample main Python program to run on top of some library.",
  "command-line": "eg_main_chrono.py [HELP] [HISTORY] [SHOW_VALID_OPTS] [VERSION] [VERBOSE_LEVEL]",
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  },
  "inputs": [
    {
      "command-line-flag": "-help",
      "description": "Show help message",
      "value-key": "[HELP]",
      "type": "Flag",
      "optional": true,
      "id": "help",
      "name": "Help Flag"
    },
    {
      "command-line-flag": "-hist",
      "description": "Show module history",
      "value-key": "[HISTORY]",
      "type": "Flag",
      "optional": true,
      "id": "history",
      "name": "History Flag"
    },
    {
      "command-line-flag": "-show_valid_opts",
      "description": "List valid options",
      "value-key": "[SHOW_VALID_OPTS]",
      "type": "Flag",
      "optional": true,
      "id": "show_valid_options",
      "name": "Show Valid Options Flag"
    },
    {
      "command-line-flag": "-ver",
      "description": "Show current version",
      "value-key": "[VERSION]",
      "type": "Flag",
      "optional": true,
      "id": "version",
      "name": "Version Flag"
    },
    {
      "command-line-flag": "-verb",
      "description": "Set the verbosity level",
      "value-key": "[VERBOSE_LEVEL]",
      "type": "String",
      "optional": true,
      "id": "verbosity_level",
      "name": "Verbosity Level"
    }
  ],
  "schema-version": "0.5",
  "url": "https://afni.nimh.nih.gov/pub/dist/doc/program_help/eg_main_chrono.py.html"
}
