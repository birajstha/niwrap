{
  "tool-version": "1.0",
  "name": "@ClustExp_CatLab",
  "author": "Justin Rajendra",
  "description": "Helper script to concatenate and label a group of data sets.",
  "command-line": "@ClustExp_CatLab -prefix [PREFIX] -input [INPUT_FILE] [HELP_FLAG]",
  "container-image": {
    "image": "your_docker_image_here:latest",
    "index": "index.docker.io",
    "type": "docker"
  },
  "inputs": [
    {
      "command-line-flag": "-prefix",
      "description": "Output file name",
      "value-key": "[PREFIX]",
      "type": "String",
      "optional": false,
      "id": "prefix",
      "name": "Prefix"
    },
    {
      "command-line-flag": "-input",
      "description": "Name of file containing the labels and data sets table (e.g. subjects.csv)",
      "value-key": "[INPUT_FILE]",
      "type": "File",
      "optional": false,
      "id": "input_file",
      "name": "Input file"
    },
    {
      "command-line-flag": "-help",
      "description": "Show help information",
      "value-key": "[HELP_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "help",
      "name": "Help Flag"
    }
  ],
  "schema-version": "0.5",
  "output-files": [
    {
      "path-template": "[PREFIX].nii.gz",
      "description": "Output data set concatenating labeled subbriks",
      "optional": false,
      "id": "output_file",
      "name": "Output file"
    }
  ]
}
