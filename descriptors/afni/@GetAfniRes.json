{
  "tool-version": "1.0.0",
  "name": "GetAfniRes",
  "author": "AFNI Team",
  "description": "Tool to return the voxel resolution of a dataset",
  "url": "https://afni.nimh.nih.gov/pub/dist/doc/program_help/@GetAfniRes.html",
  "command-line": "@GetAfniRes [OUTPUT_TYPE] [DSET]",
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  },
  "inputs": [
    {
      "description": "Output type specifying whether to return the minimum, maximum, or mean resolution",
      "value-key": "[OUTPUT_TYPE]",
      "type": "String",
      "optional": true,
      "id": "output_type",
      "name": "Output Type",
      "command-line-flag": "",
      "value-choices": [
        "-min",
        "-max",
        "-mean"
      ]
    },
    {
      "description": "Input dataset",
      "value-key": "[DSET]",
      "type": "File",
      "optional": false,
      "id": "input_dataset",
      "name": "Input dataset"
    }
  ],
  "schema-version": "0.5"
}
