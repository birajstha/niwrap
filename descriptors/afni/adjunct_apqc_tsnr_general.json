{
  "tool-version": "1.0.0",
  "name": "adjunct_apqc_tsnr_general",
  "author": "AFNI Team",
  "description": "An adjunct program for making TSNR plots for APQC.",
  "command-line": "adjunct_apqc_tsnr_general [OPTIONS]",
  "inputs": [
    {
      "command-line-flag": "-montgap",
      "description": "Specify montage gap.",
      "value-key": "[A]",
      "type": "String",
      "optional": true,
      "id": "montgap",
      "name": "Montage gap"
    },
    {
      "command-line-flag": "-montcolor",
      "description": "Specify montage color.",
      "value-key": "[A]",
      "type": "String",
      "optional": true,
      "id": "montcolor",
      "name": "Montage color"
    },
    {
      "command-line-flag": "-montx",
      "description": "Specify montage x coordinate.",
      "value-key": "[A]",
      "type": "String",
      "optional": true,
      "id": "montx",
      "name": "Montage x coordinate"
    },
    {
      "command-line-flag": "-monty",
      "description": "Specify montage y coordinate.",
      "value-key": "[A]",
      "type": "String",
      "optional": true,
      "id": "monty",
      "name": "Montage y coordinate"
    },
    {
      "command-line-flag": "-opacity",
      "description": "Specify overlay opacity.",
      "value-key": "[A]",
      "type": "String",
      "optional": true,
      "id": "opacity",
      "name": "Overlay opacity"
    },
    {
      "command-line-flag": "-blowup",
      "description": "Specify blowup factor.",
      "value-key": "[A]",
      "type": "String",
      "optional": true,
      "id": "blowup",
      "name": "Blowup factor"
    },
    {
      "command-line-flag": "-save_ftype",
      "description": "Specify save file type.",
      "value-key": "[A]",
      "type": "String",
      "optional": true,
      "id": "save_ftype",
      "name": "Save file type"
    },
    {
      "command-line-flag": "-set_dicom_xyz",
      "description": "Set DICOM x, y, z coordinates.",
      "value-key": "[A] [B] [C]",
      "type": "String",
      "list": true,
      "max-list-entries": 3,
      "optional": true,
      "id": "set_dicom_xyz",
      "name": "DICOM coordinates"
    },
    {
      "command-line-flag": "-set_ijk",
      "description": "Set IJK coordinates.",
      "value-key": "[A] [B] [C]",
      "type": "String",
      "list": true,
      "max-list-entries": 3,
      "optional": true,
      "id": "set_ijk",
      "name": "IJK coordinates"
    },
    {
      "command-line-flag": "-set_subbricks",
      "description": "Set sub-bricks.",
      "value-key": "[A] [B] [C]",
      "type": "String",
      "list": true,
      "max-list-entries": 3,
      "optional": true,
      "id": "set_subbricks",
      "name": "Sub-bricks"
    },
    {
      "command-line-flag": "-olay_alpha",
      "description": "Specify overlay alpha.",
      "value-key": "[A]",
      "type": "String",
      "optional": true,
      "id": "olay_alpha",
      "name": "Overlay alpha"
    },
    {
      "command-line-flag": "-olay_boxed",
      "description": "Specify boxed overlay.",
      "value-key": "[A]",
      "type": "String",
      "optional": true,
      "id": "olay_boxed",
      "name": "Boxed overlay"
    },
    {
      "command-line-flag": "-thr_olay",
      "description": "Specify threshold for overlay.",
      "value-key": "[A]",
      "type": "String",
      "optional": true,
      "id": "thr_olay",
      "name": "Overlay threshold"
    },
    {
      "command-line-flag": "-ulay_range_nz",
      "description": "Specify non-zero range for underlay.",
      "value-key": "[A] [B]",
      "type": "String",
      "list": true,
      "max-list-entries": 2,
      "optional": true,
      "id": "ulay_range_nz",
      "name": "Underlay range non-zero"
    },
    {
      "command-line-flag": "-ulay_range",
      "description": "Specify range for underlay.",
      "value-key": "[A] [B]",
      "type": "String",
      "list": true,
      "max-list-entries": 2,
      "optional": true,
      "id": "ulay_range",
      "name": "Underlay range"
    },
    {
      "command-line-flag": "-delta_slices",
      "description": "Specify delta slices.",
      "value-key": "[A] [B] [C]",
      "type": "String",
      "list": true,
      "max-list-entries": 3,
      "optional": true,
      "id": "delta_slices",
      "name": "Delta slices"
    },
    {
      "command-line-flag": "-olay_disc_hot_range",
      "description": "Specify discrete hot range for overlay.",
      "value-key": "[A] [B]",
      "type": "String",
      "list": true,
      "max-list-entries": 2,
      "optional": true,
      "id": "olay_disc_hot_range",
      "name": "Overlay hot range"
    },
    {
      "command-line-flag": "-olay_cont_max",
      "description": "Specify continuous max for overlay.",
      "value-key": "[A]",
      "type": "String",
      "optional": true,
      "id": "olay_cont_max",
      "name": "Overlay continuous max"
    },
    {
      "command-line-flag": "-cbar_cont",
      "description": "Specify continuous color bar.",
      "value-key": "[A]",
      "type": "String",
      "optional": true,
      "id": "cbar_cont",
      "name": "Continuous color bar"
    },
    {
      "command-line-flag": "-no_cor",
      "description": "No coronal view.",
      "type": "Flag",
      "optional": true,
      "id": "no_cor",
      "name": "No coronal view",
      "value-key": "[NO_COR]"
    },
    {
      "command-line-flag": "-no_sag",
      "description": "No sagittal view.",
      "type": "Flag",
      "optional": true,
      "id": "no_sag",
      "name": "No sagittal view",
      "value-key": "[NO_SAG]"
    },
    {
      "command-line-flag": "-no_axi",
      "description": "No axial view.",
      "type": "Flag",
      "optional": true,
      "id": "no_axi",
      "name": "No axial view",
      "value-key": "[NO_AXI]"
    },
    {
      "command-line-flag": "-echo",
      "description": "Echo the command line arguments.",
      "type": "Flag",
      "optional": true,
      "id": "echo",
      "name": "Echo",
      "value-key": "[ECHO]"
    }
  ],
  "schema-version": "0.5",
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  },
  "url": "https://afni.nimh.nih.gov/pub/dist/doc/program_help/adjunct_apqc_tsnr_general.html"
}
