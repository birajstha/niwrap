{
  "tool-version": "latest",
  "name": "3dConformist",
  "author": "AFNI",
  "description": "Program to conform a collection of datasets to the same size by zero padding",
  "command-line": "3dConformist [INPUT_FILES]",
  "container-image": {
    "type": "docker",
    "image": "afni/afni:latest",
    "index": "index.docker.io"
  },
  "inputs": [
    {
      "description": "Input datasets to be zero padded to the same size",
      "id": "input_files",
      "name": "Input files",
      "type": "File",
      "list": true,
      "optional": false,
      "value-key": "[INPUT_FILES]"
    }
  ],
  "schema-version": "0.5",
  "output-files": [
    {
      "description": "Zero padded output dataset files",
      "id": "output_files",
      "name": "Output files",
      "path-template": ".",
      "list": true,
      "optional": false
    }
  ]
}
