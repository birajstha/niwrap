{
  "tool-version": "1.0.0",
  "name": "get_afni_model_PRF",
  "author": "AFNI Team",
  "description": "A tool to get AFNI model parameters assuming a PRF framework",
  "command-line": "get_afni_model_PRF [A] [X] [Y] [SIGMA]",
  "inputs": [
    {
      "description": "Amplitude for the AFNI model",
      "value-key": "[A]",
      "type": "Number",
      "optional": false,
      "id": "amplitude",
      "name": "Amplitude"
    },
    {
      "description": "X-coordinate for the AFNI model",
      "value-key": "[X]",
      "type": "Number",
      "optional": false,
      "id": "x_coord",
      "name": "X-coordinate"
    },
    {
      "description": "Y-coordinate for the AFNI model",
      "value-key": "[Y]",
      "type": "Number",
      "optional": false,
      "id": "y_coord",
      "name": "Y-coordinate"
    },
    {
      "description": "Sigma value for the AFNI model",
      "value-key": "[SIGMA]",
      "type": "Number",
      "optional": false,
      "id": "sigma",
      "name": "Sigma"
    }
  ],
  "schema-version": "0.5",
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  },
  "url": "https://afni.nimh.nih.gov/pub/dist/doc/program_help/get_afni_model_PRF.html"
}
