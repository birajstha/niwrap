{
  "tool-version": "8",
  "name": "afni_check_omp",
  "author": "AFNI Team",
  "description": "Tool to check the OpenMP multi-threading environment for AFNI.",
  "url": "https://afni.nimh.nih.gov/pub/dist/doc/program_help/afni_check_omp.html",
  "command-line": "afni_check_omp [NUM]",
  "inputs": [
    {
      "description": "Number of iterations to run",
      "value-key": "[NUM]",
      "type": "Number",
      "optional": true,
      "id": "iterations",
      "name": "Number of iterations"
    }
  ],
  "schema-version": "0.5",
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  }
}
