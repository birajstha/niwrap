{
  "tool-version": "1.0",
  "name": "SUMA_glxdino",
  "author": "AFNI Team",
  "description": "A simple openGL test program using GLX. If it does not run, then SUMA certainly won't.",
  "url": "https://afni.nimh.nih.gov/pub/dist/doc/program_help/SUMA_glxdino.html",
  "command-line": "SUMA_glxdino [VERBOSE_FLAG]",
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  },
  "inputs": [
    {
      "command-line-flag": "-v",
      "description": "Switch on diagnostic messages",
      "value-key": "[VERBOSE_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "verbose",
      "name": "Verbose Flag"
    }
  ],
  "schema-version": "0.5"
}
