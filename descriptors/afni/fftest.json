{
  "tool-version": "1.0.0",
  "name": "fftest",
  "author": "AFNI Team",
  "description": "A command line tool for testing purposes",
  "command-line": "fftest [-q] [LEN] [NUM] [NVEC]",
  "inputs": [
    {
      "description": "Length of the test",
      "value-key": "[LEN]",
      "type": "Number",
      "optional": false,
      "id": "length",
      "name": "Length"
    },
    {
      "description": "Number of tests to run",
      "value-key": "[NUM]",
      "type": "Number",
      "optional": false,
      "id": "num_tests",
      "name": "Number of Tests"
    },
    {
      "description": "Vector size for the test",
      "value-key": "[NVEC]",
      "type": "Number",
      "optional": false,
      "id": "vector_size",
      "name": "Vector Size"
    },
    {
      "command-line-flag": "-q",
      "description": "Quiet mode",
      "value-key": "[quiet]",
      "type": "Flag",
      "optional": true,
      "id": "quiet_mode",
      "name": "Quiet Mode"
    }
  ],
  "schema-version": "0.5",
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  },
  "url": "https://afni.nimh.nih.gov/pub/dist/doc/program_help/fftest.html"
}
