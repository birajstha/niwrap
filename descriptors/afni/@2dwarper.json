{
  "tool-version": "1.0",
  "name": "2dwarper",
  "author": "Unknown",
  "description": "2D image warping tool",
  "command-line": "2dwarper [DATASET]",
  "inputs": [
    {
      "description": "Input dataset (e.g., image to be warped)",
      "value-key": "[DATASET]",
      "type": "File",
      "optional": false,
      "id": "input_dataset",
      "name": "Input dataset"
    }
  ],
  "schema-version": "0.5",
  "output-files": [
    {
      "path-template": "warped_output",
      "description": "Warped output image from the dataset",
      "optional": true,
      "id": "output_file",
      "name": "Warped output file"
    }
  ]
}
