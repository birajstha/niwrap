{
  "tool-version": "AFNI_23.1.10",
  "name": "3dnvals",
  "author": "AFNI Team",
  "description": "Tool to print the number of sub-bricks in a 3D dataset",
  "url": "https://afni.nimh.nih.gov/pub/dist/doc/program_help/3dnvals.html",
  "command-line": "3dnvals [DATASETS] [ALL_FLAG] [VERBOSE_FLAG]",
  "inputs": [
    {
      "description": "Input 3D dataset(s)",
      "value-key": "[DATASETS]",
      "type": "File",
      "list": true,
      "optional": false,
      "id": "datasets",
      "name": "Datasets"
    },
    {
      "command-line-flag": "-all",
      "description": "Print out all 4 dimensions: Nx, Ny, Nz, Nvals",
      "value-key": "[ALL_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "all_flag",
      "name": "All dimensions flag"
    },
    {
      "command-line-flag": "-verbose",
      "description": "Print the header name of the dataset first",
      "value-key": "[VERBOSE_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "verbose_flag",
      "name": "Verbose flag"
    }
  ],
  "schema-version": "0.5",
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  }
}
