{
  "tool-version": "23.1.10",
  "name": "3dABoverlap",
  "author": "AFNI Team",
  "description": "Counts various metrics about how the automasks of datasets A and B overlap or don't overlap",
  "url": "https://afni.nimh.nih.gov/pub/dist/doc/program_help/3dABoverlap.html",
  "command-line": "3dABoverlap [OPTIONS] [DATASET_A] [DATASET_B]",
  "inputs": [
    {
      "description": "First input dataset",
      "value-key": "[DATASET_A]",
      "type": "File",
      "optional": false,
      "id": "dataset_a",
      "name": "Dataset A"
    },
    {
      "description": "Second input dataset",
      "value-key": "[DATASET_B]",
      "type": "File",
      "optional": false,
      "id": "dataset_b",
      "name": "Dataset B"
    },
    {
      "command-line-flag": "-no_automask",
      "description": "Consider input datasets as masks (automask does not work on mask datasets)",
      "value-key": "[NO_AUTOMASK_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "no_automask",
      "name": "No automask"
    },
    {
      "command-line-flag": "-quiet",
      "description": "Be as quiet as possible (without being entirely mute)",
      "value-key": "[QUIET_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "quiet",
      "name": "Quiet mode"
    },
    {
      "command-line-flag": "-verb",
      "description": "Print out some progress reports (to stderr)",
      "value-key": "[VERBOSE_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "verbose",
      "name": "Verbose mode"
    }
  ],
  "schema-version": "0.5",
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  }
}
