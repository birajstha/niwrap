{
  "tool-version": "latest",
  "name": "GetAfniDims",
  "author": "AFNI Team",
  "description": "A utility tool to return dimensions of AFNI dataset",
  "command-line": "GetAfniDims [DSET]",
  "inputs": [
    {
      "description": "Input AFNI dataset",
      "value-key": "[DSET]",
      "type": "File",
      "optional": false,
      "id": "input_dset",
      "name": "Input dataset"
    }
  ],
  "schema-version": "0.5",
  "output-files": [
    {
      "path-template": "dims_output.txt",
      "description": "Text file containing the dimensions of the input dataset",
      "optional": false,
      "id": "dims_output",
      "name": "Dimensions output file"
    }
  ],
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  },
  "url": "https://afni.nimh.nih.gov/pub/dist/doc/program_help/@GetAfniDims.html"
}
