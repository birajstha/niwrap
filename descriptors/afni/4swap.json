{
  "tool-version": "1.0",
  "name": "4swap",
  "author": "Unknown",
  "description": "Swaps byte quadruples on the listed files.",
  "command-line": "4swap [-q] [FILES...]",
  "inputs": [
    {
      "description": "List of files to process.",
      "value-key": "[FILES]",
      "type": "File",
      "list": true,
      "id": "files",
      "name": "Input Files",
      "optional": false
    },
    {
      "command-line-flag": "-q",
      "description": "Work quietly; suppress output messages.",
      "value-key": "[QUIET_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "quiet",
      "name": "Quiet Flag"
    }
  ],
  "schema-version": "0.5"
}
