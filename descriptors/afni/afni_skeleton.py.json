{
  "tool-version": "1.0",
  "name": "afni_skeleton",
  "author": "R Reynolds",
  "description": "Skeleton of a basic python program example",
  "command-line": "afni_skeleton.py [INPUT_FILES] [HELP_FLAG] [HIST_FLAG] [VER_FLAG]",
  "container-image": {
    "image": "afni/afni:latest",
    "index": "index.docker.io",
    "type": "docker"
  },
  "inputs": [
    {
      "description": "Specify input files",
      "value-key": "[INPUT_FILES]",
      "type": "File",
      "list": true,
      "optional": true,
      "id": "infiles",
      "name": "Input files"
    },
    {
      "command-line-flag": "-help",
      "description": "Show help message",
      "value-key": "[HELP_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "help",
      "name": "Help flag"
    },
    {
      "command-line-flag": "-hist",
      "description": "Show the revision history",
      "value-key": "[HIST_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "hist",
      "name": "History flag"
    },
    {
      "command-line-flag": "-ver",
      "description": "Show the version number",
      "value-key": "[VER_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "ver",
      "name": "Version flag"
    }
  ],
  "schema-version": "0.5"
}
