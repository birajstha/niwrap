{
  "tool-version": "unknown",
  "name": "@IsoMasks",
  "author": "AFNI Team",
  "description": "Creates isosurfaces from isovolume envelopes.",
  "command-line": "@IsoMasks -mask [INPUT_DATASET] [ISOVALS]",
  "inputs": [
    {
      "description": "Input dataset for creating isosurfaces",
      "value-key": "[INPUT_DATASET]",
      "command-line-flag": "-mask",
      "type": "File",
      "optional": false,
      "id": "input_dataset",
      "name": "Input Dataset"
    },
    {
      "description": "Isovalue thresholds for creating isosurfaces",
      "value-key": "[ISOVALS]",
      "type": "Number",
      "list": true,
      "optional": true,
      "id": "isovals",
      "name": "Isovalues"
    }
  ],
  "schema-version": "0.5",
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  },
  "url": "https://afni.nimh.nih.gov/pub/dist/doc/program_help/@IsoMasks.html"
}
